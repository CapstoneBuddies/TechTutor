<IfModule mod_headers.c>
    # Allow CORS for all resources (images, JS, CSS, etc.)
    Header set Access-Control-Allow-Origin "*"
    Header set Access-Control-Allow-Methods "GET, POST, OPTIONS"
    Header set Access-Control-Allow-Headers "Origin, Content-Type, X-Auth-Token, X-Checksum"
    
    # Allow for image content specifically (images, etc.)
    <FilesMatch "\.(jpg|jpeg|png|gif|bmp|svg|webp)$">
        Header set Access-Control-Allow-Origin "*"
    </FilesMatch>
</IfModule>

<IfModule mod_rewrite.c>
     RewriteEngine On
     RewriteRule ^meeting_webhook\.php$ - [L]
     # Allow direct access to webhook endpoints without rewrites
     RewriteRule ^backends/handler/meeting_webhook\.php$ - [L]
     # Admin webhook management endpoints
     RewriteRule ^admin/webhooks$ backends/admin/manage_bbb_webhooks.php [QSA,L]
     RewriteRule ^admin/register-webhook$ backends/admin/register_bbb_webhook.php [QSA,L]
     RewriteRule ^admin/test-webhook$ backends/admin/test_bbb_webhook.php [QSA,L]
     RewriteRule ^admin/send-test-webhook$ backends/admin/send_test_webhook.php [QSA,L]
     
     # BigBlueButton API hooks endpoint
     RewriteRule ^bigbluebutton/api/hooks(.*)$ backends/handler/meeting_webhook.php [QSA,L]
     
     # Test endpoint for BBB Webhook
     RewriteRule ^test-bbb-hooks$ test_bbb_hooks.php [QSA,L]
     
     # Block the problematic hook/list endpoint that exposes ENV data
     RewriteRule ^hook/list$ - [F,L]
     
     # Allow direct access to webhook endpoints without restrictions
     RewriteCond %{REQUEST_URI} ^/backends/handler/meeting_webhook\.php
     RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization},L]
</IfModule>



